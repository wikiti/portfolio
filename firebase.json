{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/dotfiles",
        "destination": "https://raw.githubusercontent.com/wikiti/dotfiles/master/install.sh",
        "type": 301
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
